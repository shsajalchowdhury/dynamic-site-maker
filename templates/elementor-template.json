[
  {
    "id": "section_header",
    "elType": "section",
    "settings": {
      "layout": "full_width",
      "padding": {
        "unit": "px",
        "top": "50",
        "right": "0",
        "bottom": "50",
        "left": "0",
        "isLinked": false
      },
      "background_color": "#f8f9fa",
      "content_width": {
        "unit": "px",
        "size": 1200,
        "sizes": []
      }
    },
    "elements": [
      {
        "id": "column_header",
        "elType": "column",
        "settings": {
          "_column_size": 100,
          "padding": {
            "unit": "px",
            "top": "20",
            "right": "20",
            "bottom": "20",
            "left": "20",
            "isLinked": true
          }
        },
        "elements": [
          {
            "id": "logo_widget",
            "elType": "widget",
            "widgetType": "image",
            "settings": {
              "image": {
                "id": "{{logo_id}}",
                "url": "{{logo_url}}"
              },
              "align": "center",
              "width": {
                "unit": "%",
                "size": 25,
                "sizes": []
              },
              "space": {
                "unit": "px",
                "size": 50,
                "sizes": []
              }
            }
          },
          {
            "id": "welcome_heading",
            "elType": "widget",
            "widgetType": "heading",
            "settings": {
              "title": "Welcome to {{name}}'s Landing Page",
              "align": "center",
              "title_color": "#2c3e50",
              "typography_typography": "custom",
              "typography_font_size": {
                "unit": "px",
                "size": 42,
                "sizes": []
              },
              "typography_font_weight": "bold",
              "_margin": {
                "unit": "px",
                "top": "20",
                "right": "0",
                "bottom": "20",
                "left": "0",
                "isLinked": false
              }
            }
          }
        ]
      }
    ]
  },
  {
    "id": "section_content",
    "elType": "section",
    "settings": {
      "layout": "boxed",
      "content_width": {
        "unit": "px",
        "size": 1200,
        "sizes": []
      },
      "padding": {
        "unit": "px",
        "top": "80",
        "right": "20",
        "bottom": "80",
        "left": "20",
        "isLinked": false
      }
    },
    "elements": [
      {
        "id": "column_content",
        "elType": "column",
        "settings": {
          "_column_size": 100
        },
        "elements": [
          {
            "id": "content_text",
            "elType": "widget",
            "widgetType": "text-editor",
            "settings": {
              "editor": "<p>Thank you for visiting my landing page. This page was created using the Dynamic Site Maker plugin, which allows for quick and easy creation of custom landing pages.</p><p>Feel free to explore the content and features available on this page.</p>",
              "align": "center",
              "typography_typography": "custom",
              "typography_font_size": {
                "unit": "px",
                "size": 18,
                "sizes": []
              },
              "typography_line_height": {
                "unit": "em",
                "size": 1.6,
                "sizes": []
              }
            }
          },
          {
            "id": "divider",
            "elType": "widget",
            "widgetType": "divider",
            "settings": {
              "style": "solid",
              "weight": {
                "unit": "px",
                "size": 1,
                "sizes": []
              },
              "color": "#eaeaea",
              "width": {
                "unit": "%",
                "size": 80,
                "sizes": []
              },
              "gap": {
                "unit": "px",
                "size": 40,
                "sizes": []
              }
            }
          },
          {
            "id": "cta_heading",
            "elType": "widget",
            "widgetType": "heading",
            "settings": {
              "title": "Ready to Get Started?",
              "align": "center",
              "title_color": "#2c3e50",
              "typography_typography": "custom",
              "typography_font_size": {
                "unit": "px",
                "size": 32,
                "sizes": []
              },
              "typography_font_weight": "600",
              "_margin": {
                "unit": "px",
                "top": "0",
                "right": "0",
                "bottom": "30",
                "left": "0",
                "isLinked": false
              }
            }
          },
          {
            "id": "cta_button",
            "elType": "widget",
            "widgetType": "button",
            "settings": {
              "text": "Visit Our Affiliate Link",
              "align": "center",
              "button_text_color": "#ffffff",
              "background_color": "#3498db",
              "hover_color": "#ffffff",
              "hover_background_color": "#2980b9",
              "border_radius": {
                "unit": "px",
                "top": "4",
                "right": "4",
                "bottom": "4",
                "left": "4",
                "isLinked": true
              },
              "typography_typography": "custom",
              "typography_font_size": {
                "unit": "px",
                "size": 18,
                "sizes": []
              },
              "typography_font_weight": "600",
              "typography_text_transform": "uppercase",
              "button_padding": {
                "unit": "px",
                "top": "15",
                "right": "30",
                "bottom": "15",
                "left": "30",
                "isLinked": false
              },
              "link": {
                "url": "{{affiliate_link}}",
                "is_external": "true",
                "nofollow": "true"
              }
            }
          }
        ]
      }
    ]
  },
  {
    "id": "section_footer",
    "elType": "section",
    "settings": {
      "layout": "full_width",
      "padding": {
        "unit": "px",
        "top": "30",
        "right": "0",
        "bottom": "30",
        "left": "0",
        "isLinked": false
      },
      "background_color": "#2c3e50",
      "content_width": {
        "unit": "px",
        "size": 1200,
        "sizes": []
      }
    },
    "elements": [
      {
        "id": "column_footer",
        "elType": "column",
        "settings": {
          "_column_size": 100,
          "padding": {
            "unit": "px",
            "top": "20",
            "right": "20",
            "bottom": "20",
            "left": "20",
            "isLinked": true
          }
        },
        "elements": [
          {
            "id": "footer_text",
            "elType": "widget",
            "widgetType": "text-editor",
            "settings": {
              "editor": "<p style=\"text-align: center;\">Â© 2025 {{name}} - All Rights Reserved</p><p style=\"text-align: center;\">Created with Dynamic Site Maker</p>",
              "text_color": "#ffffff",
              "typography_typography": "custom",
              "typography_font_size": {
                "unit": "px",
                "size": 16,
                "sizes": []
              }
            }
          }
        ]
      }
    ]
  }
]
