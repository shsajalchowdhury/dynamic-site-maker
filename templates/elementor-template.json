[
  {
    "id": "main_section",
    "elType": "section",
    "settings": {
      "layout": "full_width",
      "gap": "no",
      "background_background": "classic",
      "background_color": "#000000",
      "padding": {
        "unit": "px",
        "top": "50",
        "right": "0",
        "bottom": "50",
        "left": "0",
        "isLinked": false
      }
    },
    "elements": [
      {
        "id": "main_column",
        "elType": "column",
        "settings": {
          "_column_size": 100
        },
        "elements": [
          {
            "id": "header_section",
            "elType": "section",
            "settings": {
              "structure": "30",
              "margin": {
                "unit": "px",
                "top": "0",
                "right": "0",
                "bottom": "30",
                "left": "0",
                "isLinked": false
              }
            },
            "elements": [
              {
                "id": "logo_column",
                "elType": "column",
                "settings": {
                  "_column_size": 33,
                  "_inline_size": 20
                },
                "elements": [
                  {
                    "id": "4e4fda73",
                    "elType": "widget",
                    "widgetType": "image",
                    "settings": {
                      "image": {
                        "url": "{{logo_url}}",
                        "id": "{{logo_id}}",
                        "size": "",
                        "alt": "",
                        "source": "library"
                      },
                      "width": {
                        "unit": "%",
                        "size": 100
                      }
                    }
                  }
                ]
              },
              {
                "id": "menu_column",
                "elType": "column",
                "settings": {
                  "_column_size": 33,
                  "_inline_size": 60
                },
                "elements": [
                  {
                    "id": "site_title",
                    "elType": "widget",
                    "widgetType": "heading",
                    "settings": {
                      "title": "{{name}}'s Landing Page",
                      "align": "center",
                      "title_color": "#FFFFFF",
                      "typography_typography": "custom",
                      "typography_font_family": "Roboto",
                      "typography_font_size": {
                        "unit": "px",
                        "size": 24
                      },
                      "typography_font_weight": "600"
                    }
                  }
                ]
              },
              {
                "id": "button_column",
                "elType": "column",
                "settings": {
                  "_column_size": 33,
                  "_inline_size": 20
                },
                "elements": [
                  {
                    "id": "cta_button",
                    "elType": "widget",
                    "widgetType": "button",
                    "settings": {
                      "text": "Join Now",
                      "align": "center",
                      "typography_typography": "custom",
                      "typography_font_family": "Montserrat",
                      "typography_font_size": {
                        "unit": "px",
                        "size": 15
                      },
                      "typography_font_weight": "500",
                      "button_text_color": "#FFFFFF",
                      "background_color": "#EB447D",
                      "hover_color": "#FFFFFF",
                      "hover_background_color": "#4FCBFD",
                      "button_background_hover_transition": {
                        "unit": "px",
                        "size": 0.3
                      },
                      "link": {
                        "url": "{{affiliate_link}}",
                        "is_external": "true",
                        "nofollow": "true"
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      }
    ]
  },
  {
    "id": "content_section",
    "elType": "section",
    "settings": {
      "layout": "boxed",
      "gap": "default",
      "padding": {
        "unit": "px",
        "top": "50",
        "right": "20",
        "bottom": "50",
        "left": "20",
        "isLinked": false
      }
    },
    "elements": [
      {
        "id": "content_column",
        "elType": "column",
        "settings": {
          "_column_size": 100
        },
        "elements": [
          {
            "id": "main_heading",
            "elType": "widget",
            "widgetType": "heading",
            "settings": {
              "title": "Welcome to {{name}}'s Landing Page",
              "align": "center",
              "title_color": "#333333",
              "typography_typography": "custom",
              "typography_font_family": "Roboto",
              "typography_font_size": {
                "unit": "px",
                "size": 36
              },
              "typography_font_weight": "700"
            }
          },
          {
            "id": "content_text",
            "elType": "widget",
            "widgetType": "text-editor",
            "settings": {
              "editor": "<p>{{description}}</p>",
              "align": "center",
              "typography_typography": "custom",
              "typography_font_family": "Roboto",
              "typography_font_size": {
                "unit": "px",
                "size": 18
              },
              "typography_line_height": {
                "unit": "em",
                "size": 1.6
              }
            }
          },
          {
            "id": "cta_button_main",
            "elType": "widget",
            "widgetType": "button",
            "settings": {
              "text": "Learn More",
              "align": "center",
              "typography_typography": "custom",
              "typography_font_family": "Montserrat",
              "typography_font_size": {
                "unit": "px",
                "size": 18
              },
              "typography_font_weight": "600",
              "button_text_color": "#FFFFFF",
              "background_color": "#EB447D",
              "hover_color": "#FFFFFF",
              "hover_background_color": "#4FCBFD",
              "button_background_hover_transition": {
                "unit": "px",
                "size": 0.3
              },
              "link": {
                "url": "{{affiliate_link}}",
                "is_external": "true",
                "nofollow": "true"
              }
            }
          }
        ]
      }
    ]
  }
]